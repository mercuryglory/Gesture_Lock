<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape
            android:dither="true"
            android:shape="oval" >
            <solid android:color="@color/white" />
            <stroke
                android:width="1dp"
                android:color="@color/tv_address_default" />

            <size
                android:width="70dp"
                android:height="70dp" />
        </shape >
    </item >
    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp" >
        <shape android:shape="oval" >
            <solid android:color="@color/tv_address_default" />

            <size
                android:width="30dp"
                android:height="30dp" />
        </shape >
    </item >
</layer-list >